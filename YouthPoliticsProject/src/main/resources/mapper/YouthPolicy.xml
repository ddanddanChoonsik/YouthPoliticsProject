<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.YouthPolicyMapper">

	<insert id="insertBookMark" parameterType="bookmark">
		insert into policy_member(policy_num,member_num,bookmark,policy_request,bizId)
		 values (0,7,#{bookmark},0,#{bizId})
	</insert> 
	<select id="getBookMarkCheck" parameterType="int" resultType="bookmark">
		select bookmark,bizId from policy_member where member_num=7
	</select>
	<select id="getOneBookMarkCheck" parameterType="String" resultType="int">
		select count(bizId) from policy_member where member_num=7 and bizId =#{bizId}
	</select>
	<update id="deleteBookMark" parameterType="String">
	delete from policy_member where bizId = #{bizId} and member_num =7
	</update>
</mapper>